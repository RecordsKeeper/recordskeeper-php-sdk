{
    "name": "RecordsKeeper/recordskeeper-php-sdk",
    "type": "library",
    "description": "library for PHP package recordskeeper-php-sdk.",
    "homepage": "https://github.com/RecordsKeeper/recordskeeper-php-sdk",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
             "recordskeeper-php-sdk\\": "source/"

             }
          },

   "require": {
        "php": ">=7.2.7",
        "phpunit": ">=7.2.6"
    },

    "require-dev": {
        "phpunit/phpunit": "^7.2.6"
    },
    
    "autoload-dev": {
        "psr-4": { "": "tests/"}
         }
   
}



